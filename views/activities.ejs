<h1>Look at all this</h1>

<table class="responsive-table highlight">
    <thead>
      <tr>
          <th>Name</th>
          <th>Type</th>
          <th>Distance</th>
          <th>Elevation Gain</th>
          <th>PR Count</th>
      </tr>
    </thead>
    <tbody>
      <% activity.forEach(function(activity, activityId) { %>
      <tr>
        <td><%= activity.name %></td>
        <td><%= activity.type %></td>
        <td><%= activity.distance %></td>
        <td><%= activity.total_elevation_gain %></td>
        <td><%= activity.pr_count %></td>
        <!-- this form only needs to pull the edit page, and send an ID to query params -->
        <td><a href="/edit/<%= activity.id %>">Edit</a></td>
          
      <% }); %>
    </tbody>
  </table>
